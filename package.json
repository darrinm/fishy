{
  "name": "fishy",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "fish-finder": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "web": "node dist/server/index.js",
    "web:dev": "tsc && node dist/server/index.js",
    "watch": "nodemon --watch src --watch web --ext ts,html,css,js --exec 'npm run build && node dist/server/index.js'"
  },
  "keywords": [
    "fish",
    "identification",
    "gemini",
    "ai",
    "diving",
    "video"
  ],
  "author": "",
  "license": "MIT",
  "description": "CLI tool to identify fish species in diving videos using Google Gemini AI",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@types/multer": "^2.0.0",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "fluent-ffmpeg": "^2.1.3",
    "glob": "^13.0.0",
    "multer": "^2.0.2",
    "openai": "^6.15.0",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^25.0.3",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "typescript": "^5.9.3"
  }
}
